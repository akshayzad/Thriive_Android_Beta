    <attr name="cs_disable_progress_glow" format="boolean"/>
    <attr name="cs_hide_progress_when_empty" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="SwipeableRecyclerView">
        <attr name="leftBgColor" format="color" />
        <attr name="leftImage" format="reference" />
        <attr name="leftText" format="string" />
        <attr name="rightBgColor" format="color" />
        <attr name="rightImage" format="reference" />
        <attr name="rightText" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSized" format="dimension" />
    </declare-styleable>


</resources>