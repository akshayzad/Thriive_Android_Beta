<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/rounded_background_fragment">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="270dp"
            android:layout_height="1sp"></TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="@dimen/_10dp">
            <ImageView
                android:id="@+id/img_close"
                android:layout_width="@dimen/_30dp"
                android:layout_height="@dimen/_30dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_5dp"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/_5dp"
                android:layout_marginBottom="@dimen/_5dp"
                android:src="@drawable/ic_close"></ImageView>

            <TextView
                android:id="@+id/txt_05"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto_medium"
                android:textSize="@dimen/_18dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:textColor="@color/darkGreyBlue"
                android:layout_margin="@dimen/_10dp"
                android:text=" Change Profile Photo " />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="@dimen/_20dp"
                android:layout_marginTop="@dimen/_5dp"
                android:layout_marginLeft="@dimen/_20dp"
                android:layout_marginBottom="@dimen/_20dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/txt_new"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_7dp"
                    android:padding="@dimen/_10dp"
                    android:textAlignment="center"
                    android:textColor="@color/darkGrey"
                    android:textSize="@dimen/_12dp"
                    android:fontFamily="@font/roboto_regular"
                    android:background="@drawable/outline_background_rate"
                    android:text="@string/tag_camera"
                    android:layout_gravity="center"></TextView>


                <TextView
                    android:id="@+id/txt_gallery"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_7dp"
                    android:padding="@dimen/_10dp"
                    android:textAlignment="center"
                    android:textColor="@color/darkGrey"
                    android:textSize="@dimen/_12dp"
                    android:fontFamily="@font/roboto_regular"
                    android:background="@drawable/outline_background_rate"
                    android:text="@string/tag_gallery"
                    android:layout_gravity="center"></TextView>

                <TextView
                    android:id="@+id/txt_remove"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_7dp"
                    android:padding="@dimen/_10dp"
                    android:visibility="gone"
                    android:textAlignment="center"
                    android:textColor="@color/darkGrey"
                    android:textSize="@dimen/_12dp"
                    android:fontFamily="@font/roboto_regular"
                    android:background="@drawable/outline_background_rate"
                    android:text="@string/remove_photo"
                    android:layout_gravity="center"></TextView>

            </LinearLayout>


