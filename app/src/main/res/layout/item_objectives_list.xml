<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    android:id="@+id/layout_select"
    android:background="@drawable/outline_select_experience"
    >
    <TextView
        android:id="@+id/txt_objective"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/_2dp"
        android:paddingBottom="@dimen/_2dp"
        android:paddingRight="@dimen/_2dp"
        android:paddingLeft="@dimen/_2dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="@color/darkSeaGreen"
        android:textSize="@dimen/_13dp"
        android:fontFamily="@font/roboto_regular"
        android:text="Data Analyst"
        android:layout_gravity="center"></TextView>
</LinearLayout>