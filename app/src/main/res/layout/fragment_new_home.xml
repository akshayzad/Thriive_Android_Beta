    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/refresh_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none">

            <LinearLayout
                android:id="@+id/layout_data"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10dp"
                    android:layout_marginTop="50dp"
                    android:layout_marginRight="@dimen/_10dp"
                    android:layout_marginBottom="@dimen/_10dp"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txt_11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20dp"
                        android:fontFamily="@font/roboto_regular"
                        android:text="Hi "
                        android:textColor="@color/darkGrey"
                        android:textSize="@dimen/_20dp" />

                    <TextView
                        android:id="@+id/txt_nName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20dp"
                        android:fontFamily="@font/roboto_regular"
                        android:text="Alex"
                        android:textColor="@color/darkGrey"
                        android:textSize="@dimen/_20dp" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/layout_start_journey"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_25dp"
                    android:layout_marginTop="@dimen/_30dp"
                    android:layout_marginEnd="@dimen/_25dp"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <TextView
                        android:id="@+id/txt_start_journey_header"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/roboto_medium"
                        android:text="@string/journey_title"
                        android:textColor="@color/darkGrey"
                        android:textSize="@dimen/_15dp" />

                    <TextView
                        android:id="@+id/txt_start_journey_desc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_13dp"
                        android:fontFamily="@font/roboto_regular"
                        android:text="@string/journey_desc"
                        android:textColor="@color/darkGrey"
                        android:textSize="@dimen/_13dp" />

                </LinearLayout>


                <FrameLayout
                    android:id="@+id/layout_recommended"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_25dp"
                    android:layout_marginTop="@dimen/_12dp"
                    android:layout_marginEnd="@dimen/_25dp"
                    android:visibility="gone">


                    <androidx.cardview.widget.CardView
                        android:id="@+id/recommended_card_dark"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|start"
                        android:layout_marginTop="@dimen/_12dp"
                        app:cardCornerRadius="@dimen/_10dp"
                        app:cardUseCompatPadding="true">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@color/card_color_dark"
                            android:orientation="horizontal"
                            android:padding="@dimen/_11dp">

                            <LinearLayout
                                android:id="@+id/layout_recommended_card"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_17dp"
                                android:layout_marginEnd="@dimen/_7dp"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/txt_journey_header_card"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/roboto_medium"
                                    android:text="@string/journey_title"
                                    android:textColor="@color/space_white"
                                    android:textSize="@dimen/_15dp" />

                                <TextView
                                    android:id="@+id/txt_journey_desc_card"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_13dp"
                                    android:fontFamily="@font/roboto_regular"
                                    android:text="@string/journey_desc"
                                    android:textColor="@color/space_white"
                                    android:textSize="@dimen/_13dp" />

                            </LinearLayout>

                            <ImageView
                                android:id="@+id/img_recommended"
                                android:layout_width="95dp"
                                android:layout_height="93dp"
                                android:layout_marginTop="8dp"
                                android:layout_marginEnd="@dimen/_5dp"
                                android:src="@drawable/profile_pic_card" />

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
