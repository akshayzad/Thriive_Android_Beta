                                    android:orientation="vertical"
                                    android:padding="@dimen/_5dp">

                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/_14dp">

                                        <TextView
                                            android:id="@+id/nm_name"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginRight="@dimen/_10dp"
                                            android:fontFamily="@font/roboto_medium"
                                            android:gravity="bottom"
                                            android:padding="1dp"
                                            android:text="Michael Thompson"
                                            android:textColor="@color/colorBlack"
                                            android:textSize="@dimen/_12dp" />

                                        <TextView
                                            android:id="@+id/nm_connection_count"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:layout_alignParentEnd="true"
                                            android:background="@drawable/circle_terracota_40"
                                            android:drawableEnd="@drawable/ic_group"
                                            android:drawablePadding="@dimen/_3dp"
                                            android:fontFamily="@font/roboto_medium"
                                            android:gravity="center"
                                            android:padding="1dp"
                                            android:paddingStart="@dimen/_7dp"
                                            android:paddingTop="@dimen/_5dp"
                                            android:paddingEnd="@dimen/_7dp"
                                            android:paddingBottom="@dimen/_5dp"
                                            android:text="43"
                                            android:textColor="@color/terracota"
                                            android:textSize="@dimen/_12dp"
                                            android:visibility="gone"/>

                                    </RelativeLayout>

                                    <TextView
                                        android:id="@+id/nm_reason_name"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/_14dp"
                                        android:layout_marginTop="@dimen/_5dp"
                                        android:layout_marginRight="@dimen/_10dp"
                                        android:fontFamily="@font/roboto_regular"
                                        android:padding="1dp"
                                        android:text="Meeting for Consulting"
                                        android:textColor="@color/colorBlack"
                                        android:textSize="@dimen/_12dp" />

                                    <TextView
                                        android:id="@+id/nm_meeting_tag"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="0"
                                        android:paddingTop="@dimen/_7dp"
                                        android:paddingBottom="@dimen/_7dp"
                                        android:paddingStart="@dimen/_10dp"
                                        android:layout_marginTop="@dimen/_7dp"
                                        android:layout_marginStart="@dimen/_14dp"
                                        android:paddingEnd="@dimen/_10dp"
                                        android:textAlignment="center"
                                        android:textColor="@color/black_01"
                                        android:textSize="@dimen/_10dp"
                                        android:ellipsize="middle"
                                        android:fontFamily="@font/roboto_light"
                                        android:background="@drawable/outline_background_next_meeting"
                                        android:text="Borning Inc - 1 year"
                                        android:singleLine="true" />

                                    <androidx.recyclerview.widget.RecyclerView
                                        android:id="@+id/rv_tags"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/_14dp"
                                        android:layout_marginRight="@dimen/_5dp"
                                        android:visibility="gone" />

                                    <!--<RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/_14dp"
                                        android:layout_marginTop="@dimen/_7dp"
                                        android:visibility="gone">

                                        <TextView
                                            android:id="@+id/nm_label_date"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginRight="@dimen/_10dp"
                                            android:drawableStart="@drawable/ic_calendar_15"
                                            android:drawablePadding="6dp"
                                            android:fontFamily="@font/roboto_light"
                                            android:padding="1dp"
                                            android:text="8th Jan 2020"
                                            android:textColor="@color/darkGrey"
                                            android:textSize="@dimen/_12dp" />

                                        <TextView
                                            android:id="@+id/nm_label_time"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginRight="@dimen/_3dp"
                                            android:layout_toEndOf="@id/nm_label_date"
                                            android:drawableStart="@drawable/ic_time_15"
                                            android:drawablePadding="6dp"
                                            android:fontFamily="@font/roboto_light"
                                            android:padding="1dp"
                                            android:text="10:30 - 11:00"
                                            android:textColor="@color/darkGrey"
                                            android:textSize="@dimen/_12dp" />

                                    </RelativeLayout>-->

                                </LinearLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="17dp"
                                    android:padding="@dimen/_2dp"
                                    android:layout_below="@id/nm_container">

                                    <TextView
                                        android:id="@+id/nm_label_date"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="@dimen/_10dp"
                                        android:drawableStart="@drawable/ic_calendar_15"
                                        android:drawablePadding="6dp"
                                        android:fontFamily="@font/roboto_light"
                                        android:padding="1dp"
                                        android:text="8th Jan 2020"
                                        android:textColor="@color/colorBlack"
                                        android:textSize="@dimen/_12dp" />

                                    <TextView
                                        android:id="@+id/nm_label_time"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="@dimen/_3dp"
                                        android:layout_toEndOf="@id/nm_label_date"
                                        android:drawableStart="@drawable/ic_time_15"
                                        android:drawablePadding="6dp"
                                        android:fontFamily="@font/roboto_light"
                                        android:padding="1dp"
                                        android:text="10:30 - 11:00"
                                        android:textColor="@color/colorBlack"
                                        android:textSize="@dimen/_12dp" />

                                </RelativeLayout>

                                <com.thriive.app.utilities.CircleImageView
                                    android:id="@+id/nm_img_user"
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_centerVertical="true"
                                    android:layout_alignParentEnd="true"
                                    android:layout_marginEnd="@dimen/_20dp"
                                    android:layout_margin="@dimen/_20dp"
                                    android:layout_weight="0"
                                    android:scaleType="centerInside"
                                    android:src="@drawable/user"></com.thriive.app.utilities.CircleImageView>
                            </RelativeLayout>

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </LinearLayout>


                <LinearLayout
