                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_25dp"
                    android:layout_marginTop="34dp"
                    android:layout_marginEnd="@dimen/_25dp"
                    android:background="@color/space_transparent"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/roboto_medium"
                        android:text="Recent"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_15dp" />

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_18dp"
                        android:background="@color/card_color_dark"
                        android:visibility="visible"
                        app:cardCornerRadius="@dimen/_10dp"
                        app:cardElevation="6dp"
                        app:cardUseCompatPadding="true">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/card_color_dark"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/tag_recent_objective"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_16dp"
                                    android:layout_marginTop="@dimen/_10dp"
                                    android:fontFamily="@font/roboto_medium"
                                    android:text="Seeking Advice"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_17dp" />

                                <com.google.android.flexbox.FlexboxLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="9dp"
                                    android:layout_marginEnd="38dp"
                                    android:layout_marginTop="@dimen/_5dp"
                                    android:layout_marginBottom="@dimen/_12dp"
                                    app:alignContent="stretch"
                                    app:flexWrap="wrap">

                                    <TextView
                                        android:id="@+id/tag_recent_l1attribute"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_10dp"
                                        android:layout_marginTop="9dp"
                                        android:paddingTop="6dp"
                                        android:paddingBottom="6dp"
                                        android:paddingStart="@dimen/_10dp"
                                        android:paddingEnd="@dimen/_10dp"
                                        android:fontFamily="@font/roboto_regular"
                                        android:background="@drawable/background_summary_tags"
                                        android:text="Startup Professional"
                                        android:textColor="@color/txt_tag"
                                        android:textSize="@dimen/_11dp"
                                        android:visibility="gone"/>

                                    <TextView
                                        android:id="@+id/tag_recent_persona"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_10dp"
                                        android:layout_marginTop="9dp"
                                        android:paddingTop="6dp"
                                        android:paddingBottom="6dp"
                                        android:paddingStart="@dimen/_10dp"
                                        android:paddingEnd="@dimen/_10dp"
                                        android:fontFamily="@font/roboto_regular"
                                        android:background="@drawable/background_summary_tags"
                                        android:text="Seeking Advice"
                                        android:textColor="@color/txt_tag"
                                        android:textSize="@dimen/_11dp"
                                        android:visibility="gone"/>

                                    <TextView
                                        android:id="@+id/tag_recent_l2attribute"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_10dp"
                                        android:layout_marginTop="9dp"
                                        android:paddingTop="6dp"
                                        android:paddingBottom="6dp"
                                        android:paddingStart="@dimen/_10dp"
                                        android:paddingEnd="@dimen/_10dp"
                                        android:fontFamily="@font/roboto_regular"
                                        android:background="@drawable/background_summary_tags"
                                        android:text="Seeking Advice"
                                        android:textColor="@color/txt_tag"
                                        android:textSize="@dimen/_11dp"
                                        android:visibility="gone"/>

                                    <TextView
                                        android:id="@+id/tag_recent_expertise"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_10dp"
                                        android:layout_marginTop="9dp"
                                        android:paddingTop="6dp"
                                        android:paddingBottom="6dp"
                                        android:paddingStart="@dimen/_10dp"
                                        android:paddingEnd="@dimen/_10dp"
                                        android:fontFamily="@font/roboto_regular"
                                        android:background="@drawable/background_summary_tags"
                                        android:text="Seeking Advice"
                                        android:textColor="@color/txt_tag"
                                        android:textSize="@dimen/_11dp"
                                        android:visibility="gone"/>

                                    <TextView
                                        android:id="@+id/tag_recent_domain"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_10dp"
                                        android:layout_marginTop="9dp"
                                        android:paddingTop="6dp"
                                        android:paddingBottom="6dp"
                                        android:paddingStart="@dimen/_10dp"
                                        android:paddingEnd="@dimen/_10dp"
                                        android:fontFamily="@font/roboto_regular"
                                        android:background="@drawable/background_summary_tags"
                                        android:text="Seeking Advice"
                                        android:textColor="@color/txt_tag"
                                        android:textSize="@dimen/_11dp"
                                        android:visibility="gone"/>

                                    <TextView
                                        android:id="@+id/tag_recent_subdomain"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_10dp"
                                        android:layout_marginTop="9dp"
                                        android:paddingTop="6dp"
                                        android:paddingBottom="6dp"
                                        android:paddingStart="@dimen/_10dp"
                                        android:paddingEnd="@dimen/_10dp"
                                        android:fontFamily="@font/roboto_regular"
                                        android:background="@drawable/background_summary_tags"
                                        android:text="Seeking Advice"
                                        android:textColor="@color/txt_tag"
                                        android:textSize="@dimen/_11dp"
                                        android:visibility="gone"/>

                                    <TextView
                                        android:id="@+id/tag_recent_region"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_10dp"
                                        android:layout_marginTop="9dp"
                                        android:paddingTop="6dp"
                                        android:paddingBottom="6dp"
                                        android:paddingStart="@dimen/_10dp"
                                        android:paddingEnd="@dimen/_10dp"
                                        android:fontFamily="@font/roboto_regular"
                                        android:background="@drawable/background_summary_tags"
                                        android:text="India"
                                        android:textColor="@color/txt_tag"
                                        android:textSize="@dimen/_11dp"
                                        android:visibility="gone"/>

                                </com.google.android.flexbox.FlexboxLayout>

                            </LinearLayout>

                            <ImageView
                                android:id="@+id/img_overview_objective"
                                android:layout_width="95dp"
                                android:layout_height="93dp"
                                android:layout_marginTop="@dimen/_10dp"
                                android:layout_marginEnd="@dimen/_10dp"
                                android:layout_marginBottom="@dimen/_10dp"
                                android:src="@drawable/profile_pic_card" />

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/layout_featured_thriivers"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_30dp"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_25dp"
                        android:layout_marginEnd="@dimen/_25dp"
                        android:fontFamily="@font/roboto_medium"
                        android:text="Featured Thriivers"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_15dp" />


                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_18dp"
                        android:visibility="visible"
                        app:cardCornerRadius="@dimen/_10dp"
                        app:cardElevation="6dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/paleGrey"
                            android:orientation="vertical">

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/rv_featured_thriivers"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginTop="@dimen/_12dp"
                                android:layout_marginBottom="6dp"/>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_25dp"
                                android:layout_marginEnd="@dimen/_5dp"
                                android:orientation="horizontal"
                                android:visibility="gone">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:fontFamily="@font/roboto_regular"
                                    android:text="Interested in:"
